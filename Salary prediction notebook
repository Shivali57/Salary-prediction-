ders['Job Title'].transform(['Data Scientist'])[0],\n","    'Years of Experience': 5.0\n","}])\n","\n","\n","sample_scaled = scaler.transform(sample)\n","predicted_salary = voting.predict(sample_scaled)\n","print(f\"\\nPredicted Salary (USD): {predicted_salary[0]:.2f}\")"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"_GWfEbz_WjaO","executionInfo":{"status":"ok","timestamp":1754065001501,"user_tz":-330,"elapsed":588,"user":{"displayName":"Shivali Yadav","userId":"15734708584681631141"}},"outputId":"cc49ec9e-c5b8-42a6-c166-893c46b83dca"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["\n","Predicted Salary (USD): 51248.37\n"]}]}]}